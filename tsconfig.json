{
  "compilerOptions": {
      "target": "es2017",
      "noImplicitAny": false,
      "sourceMap": true,
      "outDir": "build",
      "emitDecoratorMetadata": true,
      "experimentalDecorators": true,
      "importHelpers": true,
      "forceConsistentCasingInFileNames": true,
      "lib": [
          "es2017",
          "esnext.asynciterable"
      ],
      "moduleResolution": "node",
      "esModuleInterop": true,
  },
  "exclude": [
      "node_modules"
  ]
}